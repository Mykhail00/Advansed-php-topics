{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.3",
        "doctrine/dbal": "^3.7",
        "doctrine/orm": "^2.11",
        "symfony/cache": "^6.3",
        "doctrine/migrations": "^3.6",
        "twig/twig": "^3.7",
        "twig/intl-extra": "^3.7",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}