{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.3",
        "psr/container": "^2.0",
        "symfony/mailer": "^6.3",
        "doctrine/dbal": "^3.7",
        "doctrine/orm": "^2.11",
        "symfony/cache": "^6.3",
        "doctrine/migrations": "^3.6",
        "illuminate/database": "^10.30",
        "illuminate/events": "^10.30",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}